    "start": "vite preview",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npx prettier --cache false src --write",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "format": "npm run prettier:fix && npm run lint:fix",
    "generate-build": "tsx generate-build-version.ts",
    "build:dev": "npm run prebuild && ENV_CONFIG=development npm run generate-build && vite build --mode development",
    "build:staging": "npm run prebuild && ENV_CONFIG=staging npm run generate-build && vite build --mode staging",
